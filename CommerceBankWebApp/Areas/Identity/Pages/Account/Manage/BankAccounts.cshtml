@page
@model CommerceBankWebApp.Areas.Identity.Pages.Account.Manage.BankAccountsModel
@{
}

@foreach (var account in Model.BankAccounts)
            {
                <p>@account.AccountNumber -- @account.AccountType</p>
            }

<h1>Add Bank Account!</h1>
<form method="post">
    <div class="form-group">
        <label asp-for="Input.AccountNumber"></label>
        <input asp-for="Input.AccountNumber" class="form-control" name="AccountNumber" />
    </div>
    <div class="form-group">
    <input type="radio" asp-for="Input.AccountType" value="Checking" name="AccountType" />Checking
    <input type="radio" asp-for="Input.AccountType" value="Savings" name="AccountType" />Savings<br />
    </div>
    <button type="submit" class="btn btn-primary">Add</button>
</form>